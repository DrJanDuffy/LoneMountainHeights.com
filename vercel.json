{
  "version": 2,
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "regions": ["sfo1"],
  "env": {
    "NEXT_PUBLIC_REALSCOUT_API_KEY": "@realscout_api_key",
    "NEXT_PUBLIC_REALSCOUT_ACCOUNT_ID": "@realscout_account_id",
    "NEXT_PUBLIC_HOMEBOT_API_KEY": "@homebot_api_key",
    "NEXT_PUBLIC_HOMEBOT_ACCOUNT_ID": "@homebot_account_id",
    "NEXT_PUBLIC_CLOUDCMA_API_KEY": "@cloudcma_api_key",
    "NEXT_PUBLIC_PERCY_API_KEY": "@percy_api_key",
    "NEXT_PUBLIC_PERCY_REGION": "us"
  },
  "build": {
    "env": {
      "NEXT_TELEMETRY_DISABLED": "1"
    }
  }
} 